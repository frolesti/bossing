{
  "name": "bossing",
  "version": "0.1.0",
  "private": true,
  "description": "AplicaciÃ³ per comparar preus de supermercats i optimitzar la compra",
  "keywords": [
    "supermarket",
    "price-comparison",
    "shopping",
    "optimization"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:migrate": "pnpm --filter @bossing/database migrate",
    "db:seed": "pnpm --filter @bossing/database seed",
    "db:studio": "pnpm --filter @bossing/database studio",
    "scrape": "pnpm --filter @bossing/scrapers run scrape",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  }
}
